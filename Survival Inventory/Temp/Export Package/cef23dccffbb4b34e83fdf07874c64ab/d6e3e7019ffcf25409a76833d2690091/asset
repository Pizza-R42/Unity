//This is just an example script of how you can use the inventory from any script

using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Example : MonoBehaviour
{
    public Inventory inv;
    public Item_ apple;
    public Item_ iron;
    public Item_ gem;
    public Item_ sword;


    void Start(){
        inv.AddToInv(apple, 50);
        inv.AddToInv(apple, 500);
        inv.AddToInv(iron, 50);
        inv.AddToInv(gem, 2);
        //inv.AddAt(9, sword, 1);
    }
}
